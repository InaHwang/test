<?xml version="1.0" encoding="UTF-8"?>
<MOML>
    <THEMES>
        <THEME id="style1" style="round" padding="10" margin="5">
    </THEMES>
    
    <UILAYOUT portrait="320,460" landscape="320,460">
        <WINDOW layout="0,0,320,460" defaultImg="#ffffff">
            <LABEL layout="10,10,auto,auto" text="Hello MOML" />
            <!--<WEBVIEW layout="10,60,300,300" src="http://naver.com" controls="none"/>-->
            <CAMERA id="cam" layout="10,60,300,300" />
            <WINDOW layout="10,380,300,auto" align="linear">
                <STYLEBUTTON themaId="style1" text="open" img="#0080ff" onClick="cam.open"/>
                <STYLEBUTTON themaId="style1" text="close" img="pink" onClick="cam.close"/>
                <STYLEBUTTON themaId="style1" text="front" img="yello" onClick="cam.type='front'"/>
                <STYLEBUTTON themaId="style1" text="back" img="#orange" onClick="cam.type='back'"/>
            </WINDOW>
        </WINDOW>
    </UILAYOUT>
</MOML>
